<Window x:Class="FastInfoInput.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FastInfoInput"
        mc:Ignorable="d"
        Title="FastInfoInput" Height="350" Width="525">
    <Grid>
        <Label Content="From:" HorizontalAlignment="Left" Margin="44,85,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.398,1.143"/>
        <Label Content="To:" HorizontalAlignment="Left" Margin="281,85,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="FromTextBox" HorizontalAlignment="Left" Height="23" Margin="44,141,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" LostFocus="FromTextBox_Leave"/>
        <TextBox x:Name="ToTextBox" HorizontalAlignment="Left" Height="23" Margin="281,141,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="FLOW Interview" HorizontalAlignment="Left" Margin="159,28,0,0" VerticalAlignment="Top" FontSize="16"/>
        <Button Content="Click" HorizontalAlignment="Left" Margin="193,203,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
